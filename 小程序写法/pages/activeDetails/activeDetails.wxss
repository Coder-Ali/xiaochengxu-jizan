.weui-navbar__item{
  padding: 0;
}
.ry-act-top {
  text-align: center;
  padding-top: 40rpx;
}
.ry-act-top .publicityImg{
  width: 650rpx;
  height: 350rpx;
  margin-bottom: 20rpx;
}
.ry-act-top .i-step-line {
  background:#2d8cf0;
}
/*导航栏部分*/
.ry-steps-ot{
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  width:100%;
  background:#fff;
}
.ry-steps-ot.active{
  position: fixed;
  top: 0;
  left: 0;
  box-shadow:0 2rpx 10rpx #999;
}
.top-btn-group .txt{
  /* float: left; */
  display: block;
  width: 100rpx;
  height: 100rpx;
  border: 1px solid #bbb;
  border-radius: 50%;
  text-align: center;
  color: #999;
  line-height: 32rpx;
  padding-top: 16rpx;
  box-sizing: border-box;
  margin: 0 auto;
  font-size: 24rpx;
}
.top-btn-group .txt.last{
  padding-top: 30rpx;
}
.top-btn-group .weui-flex__item.active .txt{
  /* box-shadow:5rpx 5rpx 10rpx #0089b7; */
  color:#3cc51f;
  border:1px solid #3cc51f;
}
.top-btn-group weui-flex__item.default .txt{
  box-shadow:0;
  color:#999;
  border:1px solid #bbb;
}
.top-btn-group .weui-flex__item{
  position: relative;
}
.top-btn-group .icon{
  width: 24rpx;
  height: 44rpx;
  position: absolute;
  top: 30rpx;
  right: 0rpx;
  color: #bbb;
  margin-right: -20rpx;
}
/*导航栏部分------end*/
.gotoRL-btn{
  border-top: 1px dashed #f0f0f0;
  padding: 20rpx 0;
  margin-top: 20rpx;
  color: #999;
}
.ruleImg-ot{
  padding-top:40rpx;
  padding-bottom: 40rpx;
}
.ruleImg{
  width: 100%;
  height: 800rpx;
}
.strategy-ot{
  padding-top: 40rpx;
  padding-bottom: 40rpx;
}
.strategy-ot .top-tit{
  display: block;
  width: 100%;
  text-align: center;
  padding: 20rpx ;
  background: #f3f3f5;
  box-sizing: border-box;
  margin-bottom: 40rpx;
  color: #151517;
}
.btm-con > view:first-child{
  border-right: 1px solid #eee;
}
.btm-con > view{
  width: 49%;
  float: left;
  text-align: center;
}
.btm-con > view image{
  width: 270rpx;
  height: 202rpx;
}
.btm-con .xd{
  padding: 0 20rpx;
  box-sizing: border-box;
  text-align: justify;
}
.btm-con .xd view{
  color: #666;
  font-size: 24rpx;
  margin-top: 20rpx;
}
.gotoGzh{
  padding: 40rpx 0 60rpx;
  text-align: center;
  font-size: 24rpx;
  margin-bottom: 20rpx;
}
.gutoGzh-btn{
  color: #459ad0;
  font-size: 30rpx;
}
.gotoSC-btn{
  color: #459ad0;
  text-align: center;
  padding: 20rpx 0;
  font-size: 24rpx;
}
.scr{
  position: relative;
  height: 568px;
  background: #f2f2f2!important;
}